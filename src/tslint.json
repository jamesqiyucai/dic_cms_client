{
    "extends": "../tslint.json",
    "rules": {
      "directive-selector": [
            true,
            ["attribute", "element"],
            "app",
            "kebab-case"
        ],
      "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
      "no-trailing-whitespace": [
        true,
        "ignore-blank-lines"
      ],
      "max-line-length": false
    }
}
